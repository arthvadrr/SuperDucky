{
	"compilerOptions": {
		"target": "ES6",
		"module": "CommonJS",
		"esModuleInterop": true,
		"moduleResolution": "node",
		"strict": true,
		"forceConsistentCasingInFileNames": true,
		"resolveJsonModule": true,
		"sourceMap": true,
		"rootDir": "src",
		"outDir": "./dist"
	},
	"include": [
		"src"
	]
}